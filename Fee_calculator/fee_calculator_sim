S_0x5579dabd37b0 .scope function.vec4.s8, "calculate_expected_fee" "calculate_expected_fee" 2 52, 2 52 0, S_0x5579dab9d830;
 .timescale 0 0;
; Variable calculate_expected_fee is vec4 return value of scope S_0x5579dabd37b0
v0x5579dabd3a90_0 .var "e_time", 31 0;
v0x5579dabd3b70_0 .var "hours", 7 0;
v0x5579dabd3c60_0 .var "x_time", 31 0;
TD_Fee_calculator_tb.calculate_expected_fee ;
    %load/vec4 v0x5579dabd3c60_0;
    %load/vec4 v0x5579dabd3a90_0;
    %cmp/u;
    %jmp/0xz  T_1.4, 5;
    %pushi/vec4 10, 0, 8;
    %ret/vec4 0, 0, 8;  Assign to calculate_expected_fee (store_vec4_to_lval)
    %jmp T_1.5;
T_1.4 ;
    %load/vec4 v0x5579dabd3c60_0;
    %load/vec4 v0x5579dabd3a90_0;
    %cmp/e;
    %jmp/0xz  T_1.6, 4;
    %pushi/vec4 15, 0, 8;
    %ret/vec4 0, 0, 8;  Assign to calculate_expected_fee (store_vec4_to_lval)
    %jmp T_1.7;
T_1.6 ;
    %load/vec4 v0x5579dabd3c60_0;
    %load/vec4 v0x5579dabd3a90_0;
    %sub;
    %pushi/vec4 60, 0, 32;
    %mod;
    %cmpi/u 0, 0, 32;
    %flag_or 5, 4; GT is !LE
    %flag_inv 5;
    %jmp/0xz  T_1.8, 5;
    %load/vec4 v0x5579dabd3c60_0;
    %load/vec4 v0x5579dabd3a90_0;
    %sub;
    %pushi/vec4 60, 0, 32;
    %div;
    %addi 1, 0, 32;
    %pad/u 8;
    %store/vec4 v0x5579dabd3b70_0, 0, 8;
    %jmp T_1.9;
T_1.8 ;
    %load/vec4 v0x5579dabd3c60_0;
    %load/vec4 v0x5579dabd3a90_0;
    %sub;
    %pushi/vec4 60, 0, 32;
    %div;
    %pad/u 8;
    %store/vec4 v0x5579dabd3b70_0, 0, 8;
T_1.9 ;
    %pushi/vec4 10, 0, 32;
    %load/vec4 v0x5579dabd3b70_0;
    %pad/u 32;
    %muli 5, 0, 32;
    %add;
    %pad/u 8;
    %ret/vec4 0, 0, 8;  Assign to calculate_expected_fee (store_vec4_to_lval)
T_1.7 ;
T_1.5 ;
    %end;
