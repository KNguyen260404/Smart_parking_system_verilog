    %store/vec4 v0x5579dabd4e40_0, 0, 1;
    %delay 50, 0;
    %vpi_call 2 171 "$display", "Time=%0t: After reset - fee_amount=%0d, fee_valid=%0d", $time, v0x5579dabd51a0_0, v0x5579dabd5240_0 {0 0 0};
    %vpi_call 2 175 "$display", "\012Test Case 9: Rapid consecutive calculations" {0 0 0};
    %pushi/vec4 8000, 0, 32;
    %store/vec4 v0x5579dab93b40_0, 0, 32;
    %pushi/vec4 8060, 0, 32;
    %store/vec4 v0x5579dabd36d0_0, 0, 32;
    %pushi/vec4 8, 0, 8;
    %store/vec4 v0x5579dabd35e0_0, 0, 8;
    %fork TD_Fee_calculator_tb.calculate_and_wait, S_0x5579dab939b0;
    %join;
    %pushi/vec4 9000, 0, 32;
    %store/vec4 v0x5579dab93b40_0, 0, 32;
    %pushi/vec4 9120, 0, 32;
    %store/vec4 v0x5579dabd36d0_0, 0, 32;
    %pushi/vec4 9, 0, 8;
    %store/vec4 v0x5579dabd35e0_0, 0, 8;
    %fork TD_Fee_calculator_tb.calculate_and_wait, S_0x5579dab939b0;
    %join;
    %pushi/vec4 10000, 0, 32;
    %store/vec4 v0x5579dab93b40_0, 0, 32;
    %pushi/vec4 10180, 0, 32;
    %store/vec4 v0x5579dabd36d0_0, 0, 32;
    %pushi/vec4 10, 0, 8;
    %store/vec4 v0x5579dabd35e0_0, 0, 8;
    %fork TD_Fee_calculator_tb.calculate_and_wait, S_0x5579dab939b0;
    %join;
    %vpi_call 2 181 "$display", "\012Test Case 10: Large time values" {0 0 0};
    %pushi/vec4 1000000, 0, 32;
    %store/vec4 v0x5579dab93b40_0, 0, 32;
    %pushi/vec4 1000600, 0, 32;
    %store/vec4 v0x5579dabd36d0_0, 0, 32;
    %pushi/vec4 11, 0, 8;
    %store/vec4 v0x5579dabd35e0_0, 0, 8;
    %fork TD_Fee_calculator_tb.calculate_and_wait, S_0x5579dab939b0;
    %join;
    %delay 100, 0;
    %vpi_call 2 186 "$display", "\012Simulation completed successfully" {0 0 0};
